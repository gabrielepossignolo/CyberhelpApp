<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cyberhelp.PsychologistRegistryPage"
             Title="Cadastro - Psicólogo"
             BackgroundColor="#0277bd">

    <StackLayout>

        <Entry x:Name="entryNamePsychologist" TextColor="AliceBlue" Margin="10,35,10,5" Placeholder="Nome Completo" PlaceholderColor="#9FB6CD"/>

        <Entry x:Name="entryCity" TextColor="AliceBlue" Margin="10,10,10,5" Placeholder="Cidade" PlaceholderColor="#9FB6CD"/>

        <Entry x:Name="entryPhone" TextColor="AliceBlue" Margin="10,10,10,5" Placeholder="(XX) XXXX-XXXX" PlaceholderColor="#9FB6CD" Keyboard="Telephone"/>

        <Entry x:Name="entryCRP" TextColor="AliceBlue" Margin="10,10,10,5" Placeholder="CRP" PlaceholderColor="#9FB6CD"/>

        <Entry x:Name="entryEmail" TextColor="AliceBlue" Margin="10,10,10,5" Placeholder="E-mail" PlaceholderColor="#9FB6CD" Keyboard="Email"/>

        <Entry x:Name="entryPasswordPR" TextColor="AliceBlue" Margin="10,10,10,0" Placeholder="Senha" PlaceholderColor="#9FB6CD" IsPassword="True"/>

        <Button x:Name="btnPsychologistRegistry" Text="CADASTRAR" TextColor="AliceBlue" Margin="10,30,10,0" Style="{StaticResource StyleButtonEnter}"
               Clicked="btnPsychologistRegistry_Clicked"/>

    </StackLayout>

</ContentPage>